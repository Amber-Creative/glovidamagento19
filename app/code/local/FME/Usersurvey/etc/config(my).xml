<config>
    <modules>
        <FME_Usersurvey>
            <version>0.1.0</version>
        </FME_Usersurvey>
    </modules>
    <frontend>
        <routers>
            <usersurvey>
                <use>standard</use>
                <args>
                    <module>FME_Usersurvey</module>
                    <frontName>usersurvey</frontName>
                </args>
            </usersurvey>
        </routers>
        <layout>
            <updates>
                <usersurvey>
                    <file>usersurvey.xml</file>
                </usersurvey>
            </updates>
        </layout>
    </frontend>
    <admin>
        <routers>
			<usersurvey>
				<use>admin</use>
				<args>
					<module>FME_Usersurvey</module>
					<frontName>usersurvey</frontName>
				</args>
			</usersurvey>
        </routers>
    </admin>
    <adminhtml>
		<menu>
			<usersurvey module="usersurvey">
				<title>Usersurvey</title>
				<sort_order>71</sort_order>               
				<children>
					<items module="usersurvey">
						<title>Manage Items</title>
						<sort_order>0</sort_order>
						<action>usersurvey/adminhtml_usersurvey</action>
					</items>
				</children>
			</usersurvey>
		</menu>
		<acl>
			<resources>
				<all>
					<title>Allow Everything</title>
				</all>
				<admin>
					<children>
						<FME_Usersurvey>
							<title>Usersurvey Module</title>
							<sort_order>10</sort_order>
						</FME_Usersurvey>
					</children>
				</admin>
			</resources>
		</acl>
		<layout>
			<updates>
				<usersurvey>
					<file>usersurvey.xml</file>
				</usersurvey>
			</updates>
		</layout>
    </adminhtml>   
    <global>
        <models>
            <usersurvey>
                <class>FME_Usersurvey_Model</class>
                <resourceModel>usersurvey_mysql4</resourceModel>
            </usersurvey>
            <usersurvey_mysql4>
                <class>FME_Usersurvey_Model_Mysql4</class>
                <entities>
                    <usersurvey>
                        <table>fme_usersurvey</table>
                    </usersurvey>
                    <answers>
                        <table>fme_answers</table>
                    </answers>
                    <questionset>
                        <table>fme_questionset</table>
                    </questionset>
                </entities>
            </usersurvey_mysql4>
        </models>
        <resources>
            <usersurvey_setup>
                <setup>
                    <module>FME_Usersurvey</module>
                </setup>
                <connection>
                    <use>core_setup</use>
                </connection>
            </usersurvey_setup>
            <usersurvey_write>
                <connection>
                    <use>core_write</use>
                </connection>
            </usersurvey_write>
            <usersurvey_read>
                <connection>
                    <use>core_read</use>
                </connection>
            </usersurvey_read>
        </resources>
        <blocks>
            <usersurvey>
                <class>FME_Usersurvey_Block</class>
            </usersurvey>
        </blocks>
        <helpers>
            <usersurvey>
                <class>FME_Usersurvey_Helper</class>
            </usersurvey>
        </helpers>
    </global>
</config>